{
  "manifest_version": 2,
  "name": "Candy",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "candy 16.png",
    "48": "candy 64.png",
    "128": "candy 128.png"
  },
  "browser_action": {
    "default_icon": "candy 64.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
